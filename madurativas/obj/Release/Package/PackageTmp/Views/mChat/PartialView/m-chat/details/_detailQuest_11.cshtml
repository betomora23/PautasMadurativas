        function validaResultado() {

            if ($("#dt11_quest_1_option1").parent().hasClass("Activo")) {
                Resultado("Pasa");
                //$dt11_divAtencion.show();

            } else if ($("#dt11_quest_1_option3").parent().hasClass("Activo") || $("#dt11_quest_1_option2").parent().hasClass("Activo")) {
                $dt11_divAtencion.hide();
                if (SoloSiEnPasa() == 3 && SoloSiEnFallas() == 0) {
                    Resultado("Pasa");
                } else if (SoloSiEnPasa() > SoloSiEnFallas()) {
                    Resultado("Pasa");
                } else if (SoloSiEnPasa() == SoloSiEnFallas()) {
                    Resultado("Pasa");
                    $dt11_divAtencion.show();                    
                }else if (SoloSiEnPasa() < SoloSiEnFallas()) {
                    Resultado("Falla");
                } else if (SoloSiEnFallas() == 3 && SoloSiEnPasa() == 0) {
                    Resultado("Falla");
                } else if ((SoloSiEnFallas() == 3 && SoloSiEnPasa() == 3)) {
                    $dt11_divAtencion.show();
                    Resultado("Pasa");
                }

                if (SoloSiEnPasaNeutro() == 3 && SoloSiEnFallasNeutro() == 3) {
                    $dt11_divAtencion.hide();
                }

                //if (SoloSiEnFallas() == 3 && SoloSiEnPasa() == 0) {
