                    Resultado("Falla");
                } else if ($("#dt18_quest_4_option1").parent().hasClass("Activo") || $("#dt18_quest_4_option2").parent().hasClass("Activo")) {
                    if (CuentaRepuestasNo() == 3) {
                        Resultado("Falla");
